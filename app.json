{
  "expo": {
    "name": "Calc App",
    "slug": "nad-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Calc.png",
    "splash": {
      "image": "./assets/calc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0088CC"
