    "android": {
      "package": "com.dumbways.calcapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Calc.png",
        "backgroundColor": "#0088CC"
      }
    },
    "web": {
      "favicon": "./assets/Calc.png"
    }
  }
}
